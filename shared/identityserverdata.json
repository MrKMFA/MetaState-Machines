{
  "IdentityServerData": {
    "IdentityResources": [
      {
        "Name": "roles",
        "Enabled": true,
        "DisplayName": "Roles",
        "UserClaims": [
          "role"
        ]
      },
      {
        "Name": "openid",
        "Enabled": true,
        "Required": true,
        "DisplayName": "Your user identifier",
        "UserClaims": [
          "sub"
        ]
      },
      {
        "Name": "profile",
        "Enabled": true,
        "DisplayName": "User profile",
        "Description": "Your user profile information (first name, last name, etc.)",
        "Emphasize": true,
        "UserClaims": [
          "name",
          "family_name",
          "given_name",
          "middle_name",
          "nickname",
          "preferred_username",
          "profile",
          "picture",
          "website",
          "gender",
          "birthdate",
          "zoneinfo",
          "locale",
          "updated_at"
        ]
      },
      {
        "Name": "email",
        "Enabled": true,
        "DisplayName": "Your email address",
        "Emphasize": true,
        "UserClaims": [
          "email",
          "email_verified"
        ]
      },
      {
        "Name": "address",
        "Enabled": true,
        "DisplayName": "Your address",
        "Emphasize": true,
        "UserClaims": [
          "address"
        ]
      }
    ],
    "ApiResources": [
      {
        "Name": "metastate_identity_admin_api",
        "Scopes": [
          {
            "Name": "metastate_identity_admin_api",
            "DisplayName": "metastate_identity_admin_api",
            "Required": true,
            "UserClaims": [
              "role",
              "name"
            ]
          }
        ]
      }
    ],
    "Clients": [
      {
        "ClientId": "metastate_identity_admin",
        "ClientName": "metastate_identity_admin",
        "ClientUri": "https://admin.metastate.local",
        "AllowedGrantTypes": [
          "authorization_code"
        ],
        "RequirePkce": true,
        "ClientSecrets": [
          {
            "Value": "metastate_admin_client_secret"
          }
        ],
        "RedirectUris": [
          "https://admin.metastate.local/signin-oidc"
        ],
        "FrontChannelLogoutUri": "https://admin.metastate.local/signout-oidc",
        "PostLogoutRedirectUris": [
          "https://admin.metastate.local/signout-callback-oidc"
        ],
        "AllowedCorsOrigins": [
          "https://admin.metastate.local"
        ],
        "AllowedScopes": [
          "openid",
          "email",
          "profile",
          "roles"
        ]
      },
      {
        "ClientId": "metastate_identity_admin_api_swaggerui",
        "ClientName": "metastate_identity_admin_api_swaggerui",
        "AllowedGrantTypes": [
          "authorization_code"
        ],
        "RequireClientSecret": false,
        "RequirePkce": true,
        "RedirectUris": [
          "https://admin-api.metastate.local/swagger/oauth2-redirect.html"
        ],
        "AllowedScopes": [
          "metastate_identity_admin_api"
        ],
        "AllowedCorsOrigins": [
          "https://admin-api.metastate.local"
        ]
      }
    ]
  }
}
